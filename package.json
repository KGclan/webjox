{
  "name": "webjox",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "compose-function": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "serve": "json-server --watch db.json --port 3004",
    "eject": "react-scripts eject",
    "dev": "concurrently \"yarn serve\" \"yarn start\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/prop-types": "off",
      "camelcase": "off",
      "indent": [
        2,
        2
      ],
      "react/jsx-indent": [
        2,
        2
      ],
      "react/jsx-indent-props": [
        2,
        2
      ],
      "no-tabs": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "vars": "all"
        }
      ],
      "react/jsx-props-no-spreading": "off",
      "no-autofocus": "off",
      "max-len": [
        "error",
        {
          "code": 120
        }
      ],
      "react/no-unused-prop-types": [
        1
      ],
      "react/require-default-props": "off",
      "semi": 2,
      "quotes": [
        "error",
        "double"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/compose-function": "^0.0.30",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.3.4",
    "concurrently": "^7.6.0",
    "json-server": "^0.17.2",
    "react-router-dom": "^6.8.2",
    "tailwindcss": "^3.2.7"
  }
}
